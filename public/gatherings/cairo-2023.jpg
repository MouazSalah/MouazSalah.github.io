<svg xmlns="http://www.w3.org/2000/svg" width="400" height="250" viewBox="0 0 400 250">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#c2410c;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f59e0b;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="250" fill="url(#bg-gradient)" />
  
  <!-- Cairo skyline with pyramids -->
  <polygon points="50,180 100,120 150,180" fill="#3d3522" opacity="0.9"/>
  <polygon points="180,180 230,100 280,180" fill="#3d3522" opacity="0.9"/>
  <polygon points="300,180 330,140 360,180" fill="#3d3522" opacity="0.9"/>
  
  <!-- City silhouette -->
  <path d="M0,180 L400,180 L400,250 L0,250 Z" fill="#261e17" opacity="0.8"/>
  
  <!-- Text -->
  <text x="200" y="80" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="white" text-anchor="middle">Cairo Gathering 2023</text>
  
  <!-- Group silhouettes -->
  <circle cx="100" cy="200" r="10" fill="#ffffff" opacity="0.7"/>
  <circle cx="130" cy="200" r="10" fill="#ffffff" opacity="0.7"/>
  <circle cx="160" cy="200" r="10" fill="#ffffff" opacity="0.7"/>
  <circle cx="190" cy="200" r="10" fill="#ffffff" opacity="0.7"/>
  <circle cx="220" cy="200" r="10" fill="#ffffff" opacity="0.7"/>
  <circle cx="250" cy="200" r="10" fill="#ffffff" opacity="0.7"/>
  <circle cx="280" cy="200" r="10" fill="#ffffff" opacity="0.7"/>
  <circle cx="310" cy="200" r="10" fill="#ffffff" opacity="0.7"/>
</svg>
